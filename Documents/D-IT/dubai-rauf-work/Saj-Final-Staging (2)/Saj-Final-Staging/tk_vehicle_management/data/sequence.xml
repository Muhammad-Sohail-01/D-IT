<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="vehicle_booking_number_sequence" model="ir.sequence">
            <field name="name">Check in Sequence</field>
            <field name="code">vehicle.booking</field>
            <field name="active">TRUE</field>
            <field name="prefix">BN/VE-</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="vehicle_inspection_number_sequence" model="ir.sequence">
            <field name="name">Inspection Number</field>
            <field name="code">vehicle.inspection</field>
            <field name="active">TRUE</field>
            <field name="prefix">IN-</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="vehicle_warranty_number_sequence" model="ir.sequence">
            <field name="name">Warranty Number</field>
            <field name="code">vehicle.warranty</field>
            <field name="active">TRUE</field>
            <field name="prefix">WN/</field>
            <field name="padding">7</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>