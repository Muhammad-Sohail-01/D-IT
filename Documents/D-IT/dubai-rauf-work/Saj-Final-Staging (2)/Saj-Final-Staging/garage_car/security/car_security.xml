<?xml version="1.0" ?>
<odoo>
    <data noupdate="1">
        <record id="module_category_car_garages" model="ir.module.category">
            <field name="name">Car</field>
            <field name="sequence">17</field>
        </record>
        <record id="car_garage_group_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="module_category_car_garages"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>
        <record id="car_garage_group_manager" model="res.groups">
            <field name="name">Administrator</field>
            <field name="implied_ids" eval="[(4, ref('car_garage_group_user'))]"/>
            <field name="category_id" ref="module_category_car_garages"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>
    </data>
</odoo>
