<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="sale.action_report_saleorder" model="ir.actions.report">
            <field name="name">Inspection / Estimation</field>
            <field name="model">sale.order</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">sale_extend.report_saleorder</field>
            <field name="report_file">sale_extend.report_saleorder</field>
            <field name="print_report_name">(object.state in ('draft', 'sent') and 'Inspection - %s' % (object.name)) or 'Estimation - %s' % (object.name)</field>
            <field name="binding_model_id" ref="model_sale_order"/>
            <field name="binding_type">report</field>
        </record>
        
