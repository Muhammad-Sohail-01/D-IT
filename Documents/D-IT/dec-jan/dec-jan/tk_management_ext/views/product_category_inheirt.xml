<odoo>
    <data>
        <record id="inspection_team_view_form_inherit" model="ir.ui.view">
            <field name="model">product.category</field>
            <field name="inherit_id" ref="product.product_category_form_view"/>
            <field name="arch" type="xml">
                <!-- button addition -->
                <xpath expr="//field[@name='parent_id']" position="after">
                <button string="Price Fixed" type="object" class="btn btn-outline-success"
                                name="price_fixed" icon="fa-bookmark"/>
                    
                </xpath>
                <xpath expr="//field[@name='property_account_expense_categ_id']" position="after">
                    <field name="deferred_expense_account_id"/>
                    <field name="deferred_revenue_account_id"/>
                    <field name="deferred_journal_id"/>
                    
                    
                    
                </xpath>
                
                
            </field>
        </record>
    </data>
</odoo>